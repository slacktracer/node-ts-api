{
  "name": "proto.api",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "audit": "npm audit --audit-level=moderate",
    "build": "rm -fr ./dist && tsc",
    "compile_then_run": "tsc && node --inspect --require dotenv/config dist/main.js",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "patch": "npm version patch -m 'Build v%s' && git push --follow-tags",
    "prepatch": "npm run type-check_format-check_lint",
    "s": "nodemon",
    "start": "node main.js",
    "test": "jest",
    "test:unit": "npm test",
    "type-check": "tsc --noEmit --project ./tsconfig.json",
    "type-check_format-check_lint": "npm run type-check && npm run format-check && npm run lint",
    "type-check_lint-staged": "npm run type-check && lint-staged"
  },
  "author": "Thiago F",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "apollo-server-express": "^2.14.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.18.2",
    "fs-extra": "^9.0.1",
    "graphql-tools": "^6.0.9",
    "helmet": "^3.23.0",
    "mongoose": "^5.9.18",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "nodemon": "^2.0.4"
  }
}
